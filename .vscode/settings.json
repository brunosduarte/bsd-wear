{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#7e30ca",
    "activityBar.background": "#7e30ca",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ce8131",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#7e30ca",
    "statusBar.background": "#6426a1",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#7e30ca",
    "statusBarItem.remoteBackground": "#6426a1",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#6426a1",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#6426a199",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#6426a1"
}